<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://www.netbeans.org/ns/project/1">
    <type>org.netbeans.modules.java.j2seproject</type>
    <configuration>
        <data xmlns="http://www.netbeans.org/ns/j2se-project/3">
            <name>DGSPreviewer</name>
            <minimum-ant-version>1.6.5</minimum-ant-version>
            <source-roots>
                <root id="src.dir"/>
            </source-roots>
            <test-roots/>
        </data>
        <references xmlns="http://www.netbeans.org/ns/ant-project-references/2">
            <reference>
                <foreign-project>batik</foreign-project>
                <artifact-type>jar</artifact-type>
                <script>${build.script.reference.batik}</script>
                <target>all-jar</target>
                <clean-target>clean</clean-target>
                <id>all-jar</id>
            </reference>
            <reference>
                <foreign-project>dgs4j</foreign-project>
                <artifact-type>jar</artifact-type>
                <script>${project.dgs4j}/build.xml</script>
                <target>jar</target>
                <clean-target>clean</clean-target>
                <id>jar</id>
            </reference>
            <reference>
                <foreign-project>gif4free</foreign-project>
                <artifact-type>jar</artifact-type>
                <script>${project.gif4free}/build.xml</script>
                <target>jar</target>
                <clean-target>clean</clean-target>
                <id>jar</id>
            </reference>
        </references>
        <swingapp xmlns="http://www.netbeans.org/ns/form-swingapp/1">
            <application-class name="dgspreviewer.DGSPreviewerApp"/>
        </swingapp>
    </configuration>
</project>
